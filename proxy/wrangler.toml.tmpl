name = "llmap-proxy"
main = "src/index.ts"
compatibility_date = "2024-11-05"
compatibility_flags = [ "nodejs_compat" ]

[observability.logs]
enabled = true

[[d1_databases]]
binding = "DB" # i.e. available in your Worker on env.DB
database_name = "llmap_database"
database_id = "${DATABASE_ID}"

